@import "itemCell.wxss";


.scrollView{
    position: absolute;
    top:0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: #fff;
}
.pic-show-pop{
    position: fixed;
    top:0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .3);
    /*display: none;   */
}
.big-pic{
    text-align: center;
}
.big-pic image{
    display: inline-block;
    max-width: 95%;
